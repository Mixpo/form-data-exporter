{
  "name": "mixpo/form-data-exporter",
  "description": "Simple Export Db -> CSV library",
  "license": "MIT",
  "minimum-stability": "dev",
  "require": {
    "php" : ">=5.5",
    "ext-pdo_pgsql" : "*",
    "aws/aws-sdk-php": "2.*"
  },
  "require-dev": {
    "phpunit/phpunit": "4.6.*",
    "satooshi/php-coveralls": "0.6.1"
  },
  "autoload": {
    "psr-4": {
      "Mixpo\\Igniter\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Mixpo\\Igniter\\Test\\": "tests/"
    }
  }
}
